{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--browser=${PLAYWRIGHT_BROWSER:-msedge}"
      ],
      "env": {}
    },
    "cloudflare": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://docs.mcp.cloudflare.com/sse"
      ]
    },
    "mongodb": {
      "command": "npx",
      "args": [
        "mongo-mcp",
        "{{ mongodb_url }}{{ project_slug }}?authSource=admin"
      ]
    },
    "redis": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/redis/mcp-redis.git",
        "redis-mcp-server",
        "--url",
        "{{ redis_url }}0"
      ]
    }
  }
}
