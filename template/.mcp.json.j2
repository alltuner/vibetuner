{
  "mcpServers": {
    "playwright": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "@playwright/mcp@latest",
        "--browser=chromium"
      ]
    },
    "cloudflare": {
      "command": "npx",
      "args": [
        "mcp-remote",
        "https://docs.mcp.cloudflare.com/sse"
      ]
    },
    "mongodb": {
      "command": "npx",
      "args": [
        "mongo-mcp",
        "{{ mongodb_url }}{{ project_slug }}?authSource=admin"
      ]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["chrome-devtools-mcp@latest"]
    }
  }
}



