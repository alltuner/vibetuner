{
  devDependencies: {
    // Development dependencies
    "@tailwindcss/cli": "^4.1.7",
    concurrently: "^9.1.2",
    daisyui: "^5.0.35",
    esbuild: "^0.25.4",
    "htmx-ext-preload": "^2.1.1",
    "htmx.org": "2.0.4",
    tailwindcss: "^4.1.7",
  },
  dependencies: {
    // Dependencies
  },
  scripts: {
    "dev:css": "tailwindcss -w -i frontend/app.css -o frontend/statics/css/bundle.css",
    "dev:js": "esbuild --watch frontend/esbuild.js --bundle --sourcemap --outfile=frontend/statics/js/bundle.js",
    dev: "concurrently 'pnpm dev:css' 'pnpm dev:js'",
    "build-prod:css": "tailwindcss -i frontend/app.css --minify -o frontend/statics/css/bundle.css",
    "build-prod:js": "esbuild frontend/esbuild.js --bundle --minify --outfile=frontend/statics/js/bundle.js",
    "build-prod": "concurrently 'pnpm build-prod:css' 'pnpm build-prod:js'",
  },
}
